{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "name": "REBOCK Shoe Co",
            "num_employees": 12000,
            "description": "we make pump shoes",
            "logo_url": "https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcTmNlCD7988dILT69tR1RBEU60taQUkvFBUIg&usqp=CAU"
        }
    ],
    "required": [
        "name",
        "num_employees",
        "description",
        "logo_url"
    ],
    "properties": {
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "REBOCK Shoe Co"
            ]
        },
        "num_employees": {
            "$id": "#/properties/num_employees",
            "type": "integer",
            "title": "The num_employees schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                12000
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "we make pump shoes"
            ]
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The logo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcTmNlCD7988dILT69tR1RBEU60taQUkvFBUIg&usqp=CAU"
            ]
        }
    },
    "additionalProperties": true
}